        for i,v in pairs(game.Players.LocalPlayer:GetChildren()) do
            if v.Name == "DataLoaded" or v.Name == "DataPreloaded" then
                v:Destroy()
            end
        end
    end
end)

OrionLib:Init()

OrionLib:MakeNotification({
    Name = "Sla Hub",
    Content = "Loading Config Complete!!",
    Image = "rbxassetid://119980140458596",
    Time = 5
})
